{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"isolatedModules": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"paths": {
			"$jbx": ["src/jbx"],
			"$jbx/*": ["src/jbx/*"],
			"$stores": ["src/stores"],
			"$stores/*": ["src/stores/*"],
			"$tiles": ["src/tiles"],
			"$tiles/*": ["src/tiles/*"],
			"$components": ["src/components"],
			"$components/*": ["src/components/*"],
		}
	}
}
